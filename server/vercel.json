{
    "version": 2,
    "builds": [
      {
        "src": "server/src/app.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "server/src/controllers/**",
            "server/src/models/**"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server/src/app.ts"
      }
    ]
  }